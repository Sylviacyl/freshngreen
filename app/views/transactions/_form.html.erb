<%= simple_form_for(@transaction) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :product_id %>
    <%= f.input :payment_details %>
    <%= f.input :delivery_address %>
    <%#= f.input :stripe %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
